{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/nodemon"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
      "DB_NAME": "@db_name",
      "DB_USER": "@db_user",
      "DB_PASS": "@db_pass",
      "DB_HOST": "@db_host"
    }
}